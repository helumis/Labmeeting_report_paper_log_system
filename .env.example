# Copy to .env and edit
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres_password_here
POSTGRES_DB=labdb
POSTGRES_PORT=5432

# used by web container (since web & db in same compose, host is postgres)
DATABASE_URL=postgresql+psycopg2://postgres:postgres_password_here@postgres:5432/labdb

# secret for SessionMiddleware
SECRET_KEY=replace_with_a_random_secret_key
